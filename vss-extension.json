{
	"manifestVersion": 1,
	"id": "aks-engine",
	"name": "AKS Engine Tools",
	"description": "Extension provides AKS-Engine functionality into Azure Pipelines.",
	"version": "0.0.1",
	"publisher": "liquorichris",
	"public":  false,
	"baseUri": "https://github.com/liquorichris",
	"scopes": [],
	"tags": [
		"Kubernetes",
		"AKS-Engine",
		"K8S",
		"Azure"
	],
	"links": {
		"home": {
			"uri": "https://github.com/liquorichris/azuredevops-extension-aks-engine"
		},
		"learn": {
			"uri": "https://github.com/liquorichris/azuredevops-extension-aks-engine"
		},
		"support": {
			"uri": "https://github.com/liquorichris/azuredevops-extension-aks-engine/issues"
		},
		"repository": {
			"uri": "https://github.com/liquorichris/azuredevops-extension-aks-engine"
		}
	},
	"repository": {
		"type": "git",
		"uri": "https://github.com/liquorichris/azuredevops-extension-aks-engine"
	},
	"branding": {
		"color": "rgb(0, 114, 186)",
		"theme": "dark"
	},
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"categories": [
		"Azure Pipelines"
	],
	"icons": {
		"default": "logo.png"
	},
	"files": [
		{ "path": "Tasks/AksEngineInstaller" },
		{ "path": "Tasks/RunAksEngine" }
	],
	"content": {
		"details": {
			"path": "overview.md"
		},
		"license": {
			"path": "license.md"
		}
	},
	"contributions": [
		{
			"id": "aks-engine-installer",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/AksEngineInstaller"
			}
		},
		{
			"id": "run-aks-engine",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/RunAksEngine"
			}
		}
	]
}